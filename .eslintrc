{
  "extends": "@react-native-community",
  "rules": {
    "quotes": "off",
    "react/react-in-jsx-scope": "off",
    "react-native/no-inline-styles": "off",
    // react-hooks
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": [
      "warn", {
        "additionalHooks": "useRecoilCallback"
      }
    ]
  },
  "globals": {
    "process": true,
    "global": true
  }
}
